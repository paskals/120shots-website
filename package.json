  "dependencies": {
    "@astrojs/check": "^0.5.10",
    "@astrojs/markdoc": "^0.15.10",
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.2",
    "@tailwindcss/vite": "^4.1.0",
    "@astrojs/ts-plugin": "^1.8.0",
